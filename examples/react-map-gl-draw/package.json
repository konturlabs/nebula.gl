{
  "scripts": {
    "start": "webpack serve --open",
    "start-local": "webpack serve --open --env local",
    "build-clean": "rm -rf ./dist && mkdir dist",
    "build-static": "cp -r ./static dist/",
    "build-script": "webpack -p --env.prod",
    "build": "node scripts/validate-token.js && npm run build-clean && npm run build-static && npm run build-script"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "^7.0.19",
    "react-map-gl-draw": "^1.0.4",
    "@math.gl/core": "^3.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.0",
    "styled-components": "^5.3.6",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  }
}

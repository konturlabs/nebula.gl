{
  "description": "A suite of 3D-enabled data editing layers for deck.gl",
  "license": "MIT",
  "private": true,
  "author": "Georgios Karnas <georgios@uber.com>",
  "keywords": [
    "webgl",
    "visualization",
    "overlay",
    "layer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uber/nebula.gl"
  },
  "workspaces": [
    "modules/*",
    "examples/advanced"
  ],
  "scripts": {
    "start": "open http://nebula.gl/docs",
    "start-example": "lerna run start-local --scope=example-advance --stream",
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build --stream",
    "prettier": "prettier --write \"{modules,examples,test,dev-docs,docs,website}/**/*.{ts,tsx,js,md}\"",
    "prettier-diff": "prettier --list-different \"{modules,examples,test,dev-docs,docs,website,}/**/*.{ts,tsx,js,md}\"",
    "lint": "yarn run lint-yarn && eslint modules/**/src/**/* --ext ts,tsx,js",
    "lint-yarn": "!(grep -q unpm.u yarn.lock) && echo 'Lockfile valid.' || (echo 'Please rebuild yarn file using public npmrc' && false)",
    "types-check": "lerna run types-check --stream",
    "prepublish": "yarn run build",
    "publish-prod": "yarn run bootstrap && yarn run build && yarn run test && lerna publish --force-publish --exact && ./scripts/changelog.sh",
    "test": "yarn run lint && yarn run build && yarn run types-check && yarn run jest",
    "test-r": "yarn run lint && yarn run build",
    "test-fast": "yarn run test-node",
    "test-ci": "yarn run lint && yarn run prettier-diff && yarn run build && yarn run types-check && yarn run jest-cover",
    "jest": "jest",
    "jest-cover": "jest --coverage",
    "publish-docs": "./publish-docs.sh",
    "postinstall": "scripts/copyDeckglTypes.sh"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-export-default-from": "7.18.10",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/preset-env": "7.19.4",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@babel/register": "7.18.9",
    "@deck.gl/core": "^8.8.16",
    "@deck.gl/geo-layers": "^8.6.0",
    "@deck.gl/extensions": "^8.6.0",
    "@deck.gl/layers": "^8.6.0",
    "@deck.gl/mesh-layers": "^8.6.0",
    "@deck.gl/react": "^8.8.17",
    "@types/geojson": "^7946.0.10",
    "@types/jest": "29.2.1",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "5.1.26",
    "@types/uuid": "8.3.4",
    "@types/viewport-mercator-project": "6.1.2",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "29.2.2",
    "babel-loader": "9.0.1",
    "babel-plugin-istanbul": "6.1.1",
    "babel-plugin-version-inline": "^1.0.0",
    "core-js": "3.26.0",
    "coveralls": "^3.1.1",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-uber-es2015": "3.1.2",
    "eslint-config-uber-jsx": "3.3.3",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^4.2.5",
    "jest": "29.2.2",
    "jsdom": "^20.0.2",
    "lerna": "3.20.1",
    "module-alias": "^2.2.2",
    "pre-commit": "^1.2.2",
    "prettier": "2.7.1",
    "prettier-check": "2.0.0",
    "pretty-quick": "^3.1.3",
    "probe.gl": "^0.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "regenerator-runtime": "^0.13.10",
    "source-map-loader": "^4.0.1",
    "tap-browser-color": "^0.1.2",
    "tape": "5.6.1",
    "tape-catch": "^1.0.6",
    "transform-loader": "^0.2.4",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "indefinitely-typed": "1.1.0",
    "@deck.gl/react": "8.8.17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
